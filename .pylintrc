[MESSAGES CONTROL]
disable=
    unused-argument,
    no-member,
    missing-module-docstring,
    missing-function-docstring,
    missing-class-docstring,
    trailing-whitespace,
    line-too-long,
    wrong-import-order,
    ungrouped-imports,
    unused-import,
    unused-variable,
    broad-exception-caught,
    redefined-outer-name,
    invalid-name,
    import-error,
    no-name-in-module,
    abstract-method,
    arguments-differ,
    attribute-defined-outside-init,
    cyclic-import,
    deprecated-module,
    duplicate-code,
    expression-not-assigned,
    fixme,
    global-statement,
    import-outside-toplevel,
    inconsistent-return-statements,
    logging-format-interpolation,
    logging-not-lazy,
    method-hidden,
    missing-docstring,
    protected-access,
    signature-differs,
    too-few-public-methods,
    too-many-ancestors,
    too-many-arguments,
    too-many-branches,
    too-many-instance-attributes,
    too-many-lines,
    too-many-locals,
    too-many-public-methods,
    too-many-return-statements,
    too-many-statements,
    undefined-variable,
    unsubscriptable-object,
    unsupported-membership-test,
    used-before-assignment,
    useless-import-alias,
    useless-suppression,
    wildcard-import,
    invalid-str-returned,
    E1101,
    W0718,
    W0613,
    C0116

load-plugins=pylint_django
django-settings-module=backend.settings

[FORMAT]
max-line-length=120

[TYPECHECK]
ignored-modules=django.db.models,django.contrib.auth,django.conf,django.utils,reportlab,google
